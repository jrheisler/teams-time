{
  "manifest_version": 3,
  "name": "Teams Time",
  "version": "1.3.1",
  "description": "Quickly view team members' time zones.",
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "192": "icons/icon192.png",
    "512": "icons/icon512.png"
  }
}